{
    "name": "corneltek/phifty",
    "type": "phifty-framework",
    "require": {
        "php": ">=5.4.0",
        "corneltek/universal": "1.8.x-dev",
        "corneltek/fileutil": "^1",
        "corneltek/configkit": "^1",
        "corneltek/genphp": "^1",
        "corneltek/twig-react-directive": "1.0.x-dev",
        "corneltek/sessionkit": "~1",
        "corneltek/locale": "^2.0.0",
        "corneltek/lazyrecord": "^3.0.0",
        "corneltek/cliframework": "^3.0.0",
        "corneltek/sqlbuilder": "^3.0.0",
        "corneltek/actionkit": "^3.0.0",
        "corneltek/assetkit": "^3.4.0",
        "corneltek/pux": "2.0.x-dev",
        "phpsgi/funk": "1.0.x-dev",
        "phpsgi/phpsgi": "1.0.x-dev",
        "cypresslab/gitelephant": "^1.0",
        "twig/twig": "^1.23",
        "twig/extensions": "^1",
        "symfony/finder": "^2.7",
        "symfony/process": "^2.7",
        "symfony/yaml": "^2.7",
        "corneltek/kendo": "dev-master",
        "pimple/pimple": "^3.0",
        "corneltek/installers": "dev-master"
    },
    "suggest": {
        "swiftmailer/swiftmailer": "^5.4",
        "symfony/css-selector": "^2.7",
        "corneltek/twig-markdown": "^1"
