{
    "name": "corneltek/phifty-core",
    "require": {
        "php": ">=5.4.0",
        "corneltek/universal": "1.8.x-dev",
        "corneltek/universal-cache": "^1",
        "corneltek/fileutil": "~1",
        "corneltek/configkit": "^1",
        "corneltek/genphp": "^1",
        "corneltek/codegen": "^2",
        "corneltek/sessionkit": "~1",
        "corneltek/locale": "2.0.x-dev",
        "corneltek/lazyrecord": "3.0.x-dev",
        "corneltek/cliframework": "@dev",
        "corneltek/sqlbuilder": "@dev",
        "corneltek/actionkit": "3.0.x-dev",
        "corneltek/assetkit": "3.4.x-dev",
        "corneltek/pux": "2.0.x-dev",
        "phpsgi/funk": "1.0.x-dev",
        "phpsgi/phpsgi": "1.0.x-dev",
        "cypresslab/gitelephant": "^1.0",
        "swiftmailer/swiftmailer": "^5.4",
        "twig/twig": "^1 >=1.23",
        "twig/extensions": "^1",
        "symfony/finder": "^2.7",
        "symfony/process": "^2.7",
        "symfony/yaml": "^2.7",
        "symfony/css-selector": "^2.7",
        "corneltek/kendo": "dev-master",
        "pimple/pimple": "~3.0"
    },
    "suggest": {
        "corneltek/roller": "*",
        "corneltek/twig-markdown": "^1"
    },
    "bin": [
        "bin/phifty",
        "bin/phifty-bootstrap"
    ],
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master",
        "satooshi/php-coveralls": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Phifty\\": "Phifty/"
        }
    },
    "extra": { "branch-alias": { "dev-master": "3.0.x-dev" } }
}
