{
    "name": "phifty/phifty",
    "type": "phifty-framework",
    "require": {
        "php": ">=5.4.0",
        "phifty/locale": "^3.1.0",
        "corneltek/universal": "^1.8",
        "corneltek/fileutil": "^1",
        "corneltek/configkit": "^1.6",
        "corneltek/genphp": "^1",
        "corneltek/twig-react-directive": "^1.0",
        "corneltek/sessionkit": "~1",
        "corneltek/cliframework": "^3",
        "corneltek/lazyrecord": "^3.3.0",
        "corneltek/actionkit": "^3.1.0",
        "corneltek/assetkit": "^3.4.0",
        "corneltek/pux": "2.0.x-dev",
        "phpsgi/funk": "1.0.x-dev",
        "phpsgi/phpsgi": "1.0.x-dev",
        "cypresslab/gitelephant": "^1.0",
        "twig/twig": "^1.23",
        "twig/extensions": "^1",
        "symfony/finder": "^2.8|3.0",
        "symfony/process": "^2.8|3.0",
        "symfony/yaml": "^2.8|3.0",
        "corneltek/kendo": "2.0.x-dev",
        "pimple/pimple": "^3.0",
        "composer/installers": "dev-master",
        "swiftmailer/swiftmailer": "^5.4"
    },
    "suggest": {
        "symfony/css-selector": "^2.8|3.0",
        "predis/predis": "^1.1",
        "corneltek/twig-markdown": "^1"
    },
    "bin": ["bin/phifty"],
    "require-dev": {
        "corneltek/phpunit-testmore": "dev-master"
    },
    "license": "MIT",
    "autoload-dev":{
        "psr-4": { "App\\": "app/" }
    },
    "autoload": {
        "psr-4": { "Phifty\\": "Phifty/" }
    },
    "extra": { "branch-alias": { "dev-master": "3.1.x-dev" } }
}
